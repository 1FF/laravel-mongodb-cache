{
    "name": "1ff/laravel-mongodb-cache",
    "description": "A mongodb cache driver for laravel",
    "type": "library",
    "version": "2.12.3",
    "require": {
        "jenssegers/mongodb": "3.*",
        "illuminate/cache": "^5.8|^6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "^9.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vihren Ganev",
            "email": "vihren@appsforfit.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ForFit\\Mongodb\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ForFit\\Mongodb\\Cache\\ServiceProvider"
            ]
        }
    }
}
