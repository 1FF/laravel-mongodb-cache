{
    "name": "1ff/laravel-mongodb-cache",
    "description": "A mongodb cache driver for laravel",
    "type": "library",
    "version": "6.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/cache": "^10.0",
        "mongodb/laravel-mongodb": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vihren Ganev",
            "email": "vihren@appsforfit.com"
        },
        {
            "name": "1FF Team",
            "homepage": "https://github.com/1FF"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ForFit\\Mongodb\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ForFit\\Mongodb\\Cache\\ServiceProvider"
            ]
        }
    }
}
